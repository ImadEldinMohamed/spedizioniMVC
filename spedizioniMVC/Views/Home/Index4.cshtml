@model IEnumerable<spedizioniMVC.Models.SpedizioneAzienda>
@{
    ViewBag.Title = "Index4";
}



<h2>lista spedizioni</h2>

<h2>lista delle spedizioni azienda</h2>

<table class="table table-hover table-bordered container text-center">
    <tr>
        <th>peso</th>
        <th>dataspedzione</th>
        <th>destinazione</th>
        <th>prezzo</th>
        <th>dataconsegna</th>
        <th>IDazienda</th>


    </tr>

    @foreach (spedizioniMVC.Models.SpedizioneAzienda s in Model)
    {
        <tr>
            <td>@s.peso</td>
            <td>@s.dataspedizione</td>
            <td>@s.destinazione</td>
            <td>@s.prezzo</td>
            <td>@s.dataconsegna</td>
            <td>@s.IDazienda</td>



        </tr>
    }
</table>

<div>
    @Html.ActionLink("inserisci nuova spedizione azienda", "CreaSpedizioneAzienda")
</div>